{
  "expo": {
    "name": "YourSofia",
    "slug": "your-sofia",
    "version": "0.5.1",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sofia-gerb.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/sofia-gerb.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sofia-gerb.jpg",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "bg.sofia.yoursofia",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "bg.sofia.yoursofia",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sofia-gerb.jpg",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_MAPS_ANDROID_API_KEY}"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/sofia-gerb.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Your Sofia to use your location."
        }
      ],
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Позволете на Твоята София да използва камерата за снимки на контейнери.",
          "cameraPermission": "Позволете на Твоята София да използва камерата."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8c71d3db-2519-4b4d-a33f-4a5e3b2c2661"
      }
    }
  }
}
