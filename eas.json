{
  "cli": {
    "version": ">= 16.20.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "env": {
        "GOOGLE_MAPS_ANDROID_API_KEY": "${GOOGLE_MAPS_ANDROID_API_KEY}",
        "ANDROID_NDK_HOME": "$HOME/Library/Android/sdk/ndk/27.1.12297006"
      }
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "env": {
        "GOOGLE_MAPS_ANDROID_API_KEY": "${GOOGLE_MAPS_ANDROID_API_KEY}"
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "env": {
        "GOOGLE_MAPS_ANDROID_API_KEY": "${GOOGLE_MAPS_ANDROID_API_KEY}"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6753916395"
      }
    }
  }
}
